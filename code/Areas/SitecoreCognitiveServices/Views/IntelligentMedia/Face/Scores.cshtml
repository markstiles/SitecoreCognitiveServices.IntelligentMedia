        </div>
        <div class="score-set">
            <label>@Translator.Text("Face.Scores.HappinessLabel"): </label>
            @Html.Partial("Common/MeasureBar", Model.Happiness)
        </div>
        <div class="score-set">
            <label>@Translator.Text("Face.Scores.NeutralLabel"): </label>
            @Html.Partial("Common/MeasureBar", Model.Neutral)
        </div>
        <div class="score-set">
