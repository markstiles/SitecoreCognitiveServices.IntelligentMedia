            <label>@Translator.Text("Face.Scores.AngerLabel"): </label>
            @Html.Partial("Common/MeasureBar", Model.Anger)
        </div>
        <div class="score-set">
            <label>@Translator.Text("Face.Scores.ContemptLabel"): </label>
